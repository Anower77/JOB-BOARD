{
  "builds": [{
    "src": "job/wsgi.py",
    "use": "@vercel/python",
    "config": { 
      "maxLambdaSize": "15mb",
      "runtime": "python3.9",
      "port": 8000
    }
  }],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "job/wsgi.py"
    }
  ],
  "env": {
    "PORT": "8000"
  }
}